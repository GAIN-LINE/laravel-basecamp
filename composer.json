{
    "name": "gainline/laravel-basecamp",
    "description": "API Wrapper for Basecamp3",
    "require": {
        "php": ">=8.0",
        "ext-fileinfo": "*",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "kevinrob/guzzle-cache-middleware": "^3.0|^4.0",
        "illuminate/cache": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^6.5 || ^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Bastien",
            "email": "mbastien@coopbelvedere.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GainLine\\Basecamp\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "GainLine\\Basecamp\\BasecampServiceProvider"
            ],
            "aliases": {
                "Basecamp": "GainLine\\Basecamp\\BasecampFacade"
            }
        }
    }
}
